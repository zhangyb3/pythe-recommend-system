@charset "utf-8";

/* common */
* {
    margin: 0;
    padding: 0;
}
html {
    font-size: 62.5%;
    overflow-x: hidden;
    background-color: #ffffff;
}
html, body {
    width: 100%;
    min-height: 100%;
    position: relative;
    margin: 0 auto;
    color: #666;
    -webkit-text-size-adjust: none;
    word-wrap: break-word;
    background-color: #f3f1e5;
}
body {
    line-height: 1;
}
address, caption, cite, code, dfn, em, strong, th, var, i {
    font-style: normal;
}
table {
    border-collapse: collapse;
    border-spacing: 0;
}
caption, th {
    text-align: left;
}
h1, h2, h3, h4, h5, h6 {
    font-size: 100%;
}
input, select, textarea {
    outline: medium;
}
img, abbr, acronym, fieldset, input {
    border: 0;
    margin: 0;
}
img {
    /*max-width: 100%;*/
}
ul, ol {
    list-style-image: none;
    list-style-type: none;
    list-style-position: inside;
}
ul, li {
    list-style-type: none;
}

a:hover, a:focus, a:link, a:visited {
    text-decoration: none;
    color: rgb(51,51,51);
}
.clear {
    clear: both;
}
.clearfix:after {
    content: ".";
    display: block;
    height: 0;
    clear: both;
    visibility: hidden;
}
@media screen and (max-width:359px) and (min-width:320px) {
    html,body{
        font-size: 10.7px !important;
    }
    .web-download-bar .img img{
        width:40px;
        height: 71px;
    }
    .web-download-bar .download .download-img img{
        width: 40px;
        height: 20px;
    }
}
@media screen and (max-width:374px) and (min-width:360px) {
    html,body{
        font-size: 12px !important;
    }
    .web-download-bar .img img{
        width:40px;
        height: 73px;
    }
    .web-download-bar .download .download-img img{
        width: 50px;
        height: 20px;
    }
}
@media screen and (max-width:413px) and (min-width:375px) {
    html,body{
        font-size: 12.5px !important;
    }
    .web-download-bar .img img{
        width:40px;
        height: 75px;
    }
    .web-download-bar .download .download-img img{
        width: 50px;
        height: 20px;
    }
}
@media screen and (max-width:749px)  and (min-width:414px){
    html,body{
        font-size: 13.8px !important;
    }
    .web-download-bar .img img{
        width:40px;
        height: 80px;
    }
    .web-download-bar .download .download-img img{
        width: 50px;
        height: 25px;
    }
}@media screen and (min-width:750px) {
    html,body{
        font-size: 25px !important;
    }
    .web-download-bar .img img{
        width:62px;
        height: 130px;
    }
    .web-download-bar .download .download-img img{
        width: 78px;
        height: 36px;
    }
}

/** float **/
.left {
    float: left;
}
.right {
    float: right;
}
/*download*/
.web-download-bar{
    height: 4.4rem;
    width: 100%;
    background-color: #202020;
    opacity:0.85;
    position: fixed;
    bottom: 0;
    padding-bottom: 0.64rem;
}
.web-download-bar .img {
    position: relative;
    margin-top:-1.4rem;
    margin-left:0.72rem;
}
.web-download-bar h1{
    font-size:2rem;
    color: #fff;
    margin-left:1.12rem;
    margin-top:10px;
}
.web-download-bar .txt{
    font-size:1rem;
    color:#fff;
    margin-top:9px;
    margin-left:1.12rem;
}
.web-download-bar .go-download{
    height: 2.8rem;
    width: 7.6rem;
    background-color: #65bfff;
    font-size:1.36rem;
    text-align: center;
    line-height:2.8rem;
    border-radius: 5px;
    margin-top:0.8rem;
    margin-right:0.68rem;
    color: #fff;
}
.web-download-bar .download-img{
    margin-right:0.8rem;
    margin-top:1.6rem;
}
/*content*/
.title{
    text-align: center;
    margin-top:2.72rem;
}
.title h3{
    color: #000;
    font-size: 2rem;
    font-weight: 600;
}
.title p{
    font-size:1rem;
    margin-top:0.88rem;
}
.content{
    margin-top:0.8rem;
		padding-right:10%;
	padding-left:10%;
}
.content p{
    color:#666666;
	text-align:justify;
	text-justify:distribute;
	font-size:100px;
	font-family:微软雅黑;
	line-height: 1.5;
	padding:10px 0;

}
.appreciate{
    margin-top:8.8rem;
    padding: 0 1.84rem;
}
.appreciate h3{
    font-size:1.28rem;
    font-weight:700;
}
.appreciate .detail{
    color:#666666;
	text-align:justify;
	text-justify:distribute;
	font-size:21px;
	font-family:微软雅黑;
	line-height: 1.5;
}